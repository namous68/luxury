{% extends 'base.html.twig' %}

{% block title %}Candidature{% endblock %}

{% block body %}
    <h1>Candidature</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ candidature.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ candidature.name }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ candidature.email }}</td>
            </tr>
            <tr>
                <th>TitreOffer</th>
                <td>{{ candidature.titreOffer }}</td>
            </tr>
            <tr>
                <th>SocietyName</th>
                <td>{{ candidature.societyName }}</td>
            </tr>
            <tr>
                <th>Contact</th>
                <td>{{ candidature.contact }}</td>
            </tr>
            <tr>
                <th>ContactMail</th>
                <td>{{ candidature.contactMail }}</td>
            </tr>
            <tr>
                <th>DepositeDate</th>
                <td>{{ candidature.DepositeDate ? candidature.DepositeDate|date('Y-m-d') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('app_candidature_crud_index') }}">back to list</a>

    <a href="{{ path('app_candidature_crud_edit', {'id': candidature.id}) }}">edit</a>

    {{ include('candidature_crud/_delete_form.html.twig') }}
{% endblock %}
